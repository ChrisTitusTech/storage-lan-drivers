;------------------------------------------------------------------------------
;  Windows 2000 / XP Sections
;------------------------------------------------------------------------------
;VN3300
;---------
[DI_vn3300.NTamd64]
CopyFiles=@vn3300.sys
AddReg=vn3300.registry,options.registry

[DI_vn3300.NTamd64.Services]
AddService=vn3300,2,vn3300.Service

;Common
;------
[vn3300.Service]
ServiceType=1
StartType=3
ErrorControl=1
ServiceBinary=%12%\vn3300.sys

