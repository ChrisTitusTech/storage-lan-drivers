; pcip device Install Section
;*****************************************
[SmartPqi_Proxy_Install.ntamd64]
Include=wpcip.inf
Needs=pcip_Device.ntamd64

[SmartPqi_Proxy_Install.ntamd64.HW]
Include=wpcip.inf
Needs=pcip_Device.ntamd64.HW
AddReg=Supported_Device.ntamd64.AddReg

[SmartPqi_Proxy_Install.ntamd64.Services]
Include=wpcip.inf
Needs=pcip_Device.ntamd64.Services
AddService = pcip, %SPSVCINST_ASSOCSERVICE%, SmartPqi_Proxy_Service_Install

[SmartPqi_Proxy_Service_Install]
DisplayName    = %pcip.SVCDESC%
ServiceType    = 1               ; SERVICE_KERNEL_DRIVER
StartType      = 3               ; SERVICE_DEMAND_START
ErrorControl   = 1               ; SERVICE_ERROR_NORMAL
ServiceBinary  = %12%\pcip.sys
LoadOrderGroup = Extended Base

[SmartPqi_Proxy_Install.ntamd64.Interfaces]
Include=wpcip.inf
Needs=pcip_Device.ntamd64.Interfaces

[Supported_Device.ntamd64.AddReg]
HKR,,SupportedDevice, 0x00010001, 0x1

