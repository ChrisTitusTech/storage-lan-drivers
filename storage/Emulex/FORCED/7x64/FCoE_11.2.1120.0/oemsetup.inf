;
; The Install Section
;

[elxcna_MSI]
CopyFiles = elxcna_CopyFiles
AddReg    = elxcna_Registry_Update

[elxcna_MSI.HW]
Include = machine.inf
Needs   = PciIoSpaceNotRequired
AddReg  = elxcna_Registry_Update_MSI

[elxcna_BE_MSI]
CopyFiles = elxcna_CopyFiles
AddReg    = elxcna_Registry_Update

[elxcna_BE_MSI.HW]
Include = machine.inf
Needs   = PciIoSpaceNotRequired
AddReg  = elxcna_Registry_Update_BE_MSI

[elxcna_Lancer_MSI]
CopyFiles = elxcna_CopyFiles
AddReg    = elxcna_Registry_Update

[elxcna_Lancer_MSI.HW]
Include = machine.inf
Needs   = PciIoSpaceNotRequired
AddReg  = elxcna_Registry_Update_Lancer_MSI

;
; diskid = description,tagfile,unused,subdir
;
[SourceDisksNames]
1 = %elxcna.DeviceDesc%,lpscsi,,

;
; filename_on_source = diskid,subdir,size
;
[SourceDisksFiles]
elxcna.sys  = 1
elxcna.dll  = 1

[elxcna_CopyFiles]
elxcna.sys

[elxcna_Registry_Update]
HKLM,SYSTEM\CurrentControlSet\Services\disk,TimeOutValue,0x00010001,0x3c
HKLM,SYSTEM\CurrentControlSet\Services\elxcna\Parameters\PnPInterface,"5",0x00010001, 0x00000001
HKLM,SYSTEM\CurrentControlSet\Services\elxcna\Parameters,BusType,0x00010001,0x0000006
HKLM,SYSTEM\CurrentControlSet\Services\elxcna\Parameters,Supports64BitAddressing,0x00010001,0x0000001

[elxcna_Registry_Update_MSI]
HKR,"Interrupt Management\MessageSignaledInterruptProperties", MSISupported,       0x00010001, 1
HKR,"Interrupt Management\MessageSignaledInterruptProperties", MessageNumberLimit, 0x00010001, 8

[elxcna_Registry_Update_BE_MSI]
HKR,"Interrupt Management\Affinity Policy", GroupPolicy, 0x00010001, 1

HKR,"Interrupt Management\MessageSignaledInterruptProperties", MSISupported,       0x00010001, 1
HKR,"Interrupt Management\MessageSignaledInterruptProperties", MessageNumberLimit, 0x00010001, 32

HKR,"Interrupt Management\MessageSignaledInterruptProperties\Range\0", DevicePolicy,    0x00010001, 5
HKR,"Interrupt Management\MessageSignaledInterruptProperties\Range\0", StartingMessage, 0x00010001, 0
HKR,"Interrupt Management\MessageSignaledInterruptProperties\Range\0", EndingMessage,   0x00010001, 31

[elxcna_Registry_Update_Lancer_MSI]
HKR,"Interrupt Management\Affinity Policy", GroupPolicy, 0x00010001, 1

HKR,"Interrupt Management\MessageSignaledInterruptProperties", MSISupported,       0x00010001, 1
HKR,"Interrupt Management\MessageSignaledInterruptProperties", MessageNumberLimit, 0x00010001, 32

HKR,"Interrupt Management\MessageSignaledInterruptProperties\Range\0", DevicePolicy,    0x00010001, 5
HKR,"Interrupt Management\MessageSignaledInterruptProperties\Range\0", StartingMessage, 0x00010001, 0
HKR,"Interrupt Management\MessageSignaledInterruptProperties\Range\0", EndingMessage,   0x00010001, 31

[elxcna_MSI.Services]
AddService = elxcna, %SPSVCINST_ASSOCSERVICE%, elxcna_Service_Inst, Miniport_EventLog_Inst

[elxcna_BE_MSI.Services]
AddService = elxcna, %SPSVCINST_ASSOCSERVICE%, elxcna_Service_Inst, Miniport_EventLog_Inst

[elxcna_Lancer_MSI.Services]
AddService = elxcna, %SPSVCINST_ASSOCSERVICE%, elxcna_Service_Inst, Miniport_EventLog_Inst

[elxcna_Service_Inst]
ServiceType    = %SERVICE_KERNEL_DRIVER%
StartType      = %SERVICE_BOOT_START%
ErrorControl   = %SERVICE_ERROR_NORMAL%
ServiceBinary  = %12%\elxcna.sys
LoadOrderGroup = SCSI Miniport

[Miniport_EventLog_Inst]
AddReg = Miniport_EventLog_AddReg

[Miniport_EventLog_AddReg]
HKR,,EventMessageFile,%REG_EXPAND_SZ%,"%%SystemRoot%%\System32\IoLogMsg.dll"
HKR,,TypesSupported,%REG_DWORD%,7

;*******************************************************************************
;
; Co-Installer Section
;
;*******************************************************************************

[elxcna_MSI.CoInstallers]   
CopyFiles = CoInstaller.CopyFiles
AddReg    = CoInstaller.AddReg

[elxcna_BE_MSI.CoInstallers]   
CopyFiles = CoInstaller.CopyFiles
AddReg    = CoInstaller.AddReg

[elxcna_Lancer_MSI.CoInstallers]   
CopyFiles = CoInstaller.CopyFiles
AddReg    = CoInstaller.AddReg

[CoInstaller.AddReg]
HKR,,CoInstallers32,0x00010000,"elxcna.dll,elxcna";

[CoInstaller.CopyFiles]
;0x00000020 - do not copy if target is newer
;0x00000040 - leave target alone if version is same as source
elxcna.dll,,,0x00000060

[Strings]
;Localizable strings
Msft               = "Microsoft"
EMULEX             = "Emulex"
elxcna.DeviceDesc = "Emulex FCoE HBA - Storport Miniport Driver"

;Non-Localizable strings
SPSVCINST_ASSOCSERVICE = 0x00000002
SERVICE_KERNEL_DRIVER  = 1
SERVICE_BOOT_START     = 0
SERVICE_ERROR_NORMAL   = 1
REG_EXPAND_SZ          = 0x00020000
REG_DWORD              = 0x00010001
REG_SZ                 = 0x00000000
