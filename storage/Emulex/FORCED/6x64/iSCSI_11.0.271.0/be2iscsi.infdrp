HKR,Params\im_policy\enum,2,,"Moderate"
HKR,Params\im_policy\enum,3,,"Conservative"
HKR,Params\im_policy\enum,4,,"Very conservative"

HKR,Params\large_io,ParamDesc,,"%large_io%"
HKR,Params\large_io,Default,,"0"
HKR,Params\large_io,Type,,"enum"
HKR,Params\large_io\enum,0,,"64"
HKR,Params\large_io\enum,1,,"512"

HKR,Params\ldto,ParamDesc,,"%ldto%"
HKR,Params\ldto,Default,,"20"
HKR,Params\ldto,Type,,"long"
HKR,Params\ldto,Min,,"0"
HKR,Params\ldto,Max,,"3600"

