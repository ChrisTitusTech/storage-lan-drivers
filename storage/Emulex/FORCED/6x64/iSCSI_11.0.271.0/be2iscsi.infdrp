[be2iscsi_Install]
CopyFiles = MiniportDestDir_CopyFiles,MiniportDestDir_CopyFiles.DLL
AddReg = PropPage

[PropPage]
HKR,,EnumPropPages32,,"bepiscsi.dll,PropPage_bepiscsi"



[be2iscsi_Install.HW]
AddReg = be2iscsi_msi_reg

[be2iscsi_msi_reg]
HKR, Interrupt Management,,%REG_CREATEKEY%
HKR, Interrupt Management\MessageSignaledInterruptProperties,,%REG_CREATEKEY%
HKR, Interrupt Management\MessageSignaledInterruptProperties, MSISupported, %REG_DWORD%, 1
HKR, Interrupt Management\MessageSignaledInterruptProperties, MessageNumberLimit, %REG_DWORD%, 32
HKR, Interrupt Management\Affinity Policy, DevicePolicy, %REG_DWORD%, %IrqPolicySpreadMessagesAcrossAllProcessors%
HKR, Interrupt Management\Affinity Policy, GroupPolicy, %REG_DWORD%, 1


[be2iscsi_Install.Services]
AddService = be2iscsi, %SPSVCINST_ASSOCSERVICE%, be2iscsi_Service_Install, EventLog_Install

[be2iscsi_Service_Install]
ServiceType    = %SERVICE_KERNEL_DRIVER%
StartType      = %SERVICE_BOOT_START%
ErrorControl   = %SERVICE_ERROR_NORMAL%
ServiceBinary  = %12%\be2iscsi.sys
LoadOrderGroup = SCSI Miniport
AddReg    = pnpsafe_pci_reg,iscsi_driver_parameters

[MiniportDestDir_CopyFiles]
be2iscsi.sys

[MiniportDestDir_CopyFiles.DLL]
bepiscsi.dll,,,0x00004002

[pnpsafe_pci_reg]
HKR, "Parameters\PnpInterface", "5", 0x00010001, 0x00000001
HKR, Parameters,BusType,0x00010001,0x9
;Do not require users to reboot
HKR, Parameters\Device,DriverParameters,0x00000000

[iscsi_driver_parameters]


HKR,Params\eto,ParamDesc,,"%eto%"
HKR,Params\eto,Default,,"90"
HKR,Params\eto,Type,,"long"
HKR,Params\eto,Min,,"0"
HKR,Params\eto,Max,,"3600"


