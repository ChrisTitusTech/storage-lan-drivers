ibmtpft2k12.sys

[ibmtp2k12_Inst.Services]
AddService = ibmtp2k12, %SPSVCINST_ASSOCSERVICE%, ibmtp2k12_Service_Inst, ibmtp2k12_EventLog_Inst
AddService = IBMTpFt2k12,,IBMTpFt2k12_Service_Inst

[IBMTpFt2k12_Service_Inst]
DisplayName   = %IBMTpFt.SVCDESC%
ServiceType   = 1
StartType     = 3
ErrorControl  = 1
ServiceBinary = %12%\IBMTpFt2k12.sys
AddReg        = SystemDrive_addreg

[SystemDrive_addreg]
HKR,,"SystemDrive",%REG_SZ%,%24%

[ibmtp2k12_Service_Inst]
ServiceType    = %SERVICE_KERNEL_DRIVER%
StartType      = %SERVICE_DEMAND_START%
ErrorControl   = %SERVICE_ERROR_NORMAL%
ServiceBinary  = %12%\ibmtp2k12.sys
AddReg         = NotifyFailover_AddReg

[NotifyFailover_AddReg]
HKR,,"FailoverDriver", 0x00010001,0x1
HKR,,"SystemDrive",%REG_SZ%,%24%

[ibmtp2k12_EventLog_Inst]
AddReg = ibmtp2k12_EventLog_AddReg

[ibmtp2k12_EventLog_AddReg]
HKR,,EventMessageFile,%REG_EXPAND_SZ%,"%%SystemRoot%%\System32\IoLogMsg.dll;%%SystemRoot%%\System32\drivers\ibmtp2k12.sys"
; enclosing quotes on preceding entry prevent semicolon from being interpreted as a comment
HKR,,TypesSupported,%REG_DWORD%,7

