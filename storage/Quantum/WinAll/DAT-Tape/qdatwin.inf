[stdat_Inst.NTia64]
UseInfDeviceDesc=1
CopyFiles = @qdatwin.sys

[stdat_Inst.NTia64.Services]
AddService = qdatwin, %SPSVCINST_ASSOCSERVICE%, stdat_Service_Inst, stdat_EventLog_Inst

; ---- Start CoInstaller installation -----------------------------------------

[stdat_Inst.NTia64.CoInstallers]
CopyFiles = CoCopy_ia64
AddReg    = CoAddReg_ia64

[CoCopy_ia64]
qdatwin.dll,,,0x10

[CoAddReg_ia64]
HKR,,CoInstallers32,0x00010000,"qdatwin.dll,qdatwin_CoInstaller"

; ---- End CoInstaller installation -----------------------------------------


[stdat_Service_Inst]
ServiceType    = %SERVICE_KERNEL_DRIVER%
StartType      = %SERVICE_DEMAND_START%
ErrorControl   = %SERVICE_ERROR_NORMAL%
ServiceBinary  = %12%\qdatwin.sys

[stdat_EventLog_Inst]
AddReg = stdat_EventLog_AddReg

[stdat_EventLog_AddReg]
HKR,,EventMessageFile,%REG_EXPAND_SZ%,"%%SystemRoot%%\System32\IoLogMsg.dll;%%SystemRoot%%\System32\drivers\qdatwin.sys"
HKR,,TypesSupported,%REG_DWORD%,7

; source-disk-number = disk-label,tag-or-cab-file,,subdir,flags,tag-file

[SourceDisksNames.x86]
1 = %sourcediskname%,,,\i386

[SourceDisksNames.amd64]
1 = %sourcediskname%,,,\amd64

[SourceDisksNames.ia64]
1 = %sourcediskname%,,,\ia64

; source-filename = source-disk-number,subdir,size

[SourceDisksFiles]
qdatwin.sys = 1
qdatwin.dll = 1

[DestinationDirs]
DefaultDestDir = 12 ; system32\drivers
CoCopy_x86     = 11 ; system32
CoCopy_amd64   = 11 ; system32
CoCopy_ia64    = 11 ; system32

[Strings]
; Non-localizable
SPSVCINST_ASSOCSERVICE = 0x00000002
SERVICE_KERNEL_DRIVER  = 1
SERVICE_DEMAND_START   = 3
SERVICE_ERROR_NORMAL   = 1
REG_EXPAND_SZ          = 0x00020000
REG_DWORD              = 0x00010001

QuantumCorp = "Quantum Corporation"
Quantum = "Quantum"

; Localizable
sourcediskname = "DAT Tape Driver for Windows 2000/XP/2003/Vista"
stdat_DeviceDesc = "Quantum DAT Tape Drive"
ibmdat_DeviceDesc = "IBM DDS Gen5 Tape Drive"
